<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
		PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
		"http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
	<properties resource="./generator/mysql/generator.properties" />
	<context id="lambda-mls" targetRuntime="MyBatis3">
		<property name="beginningDelimiter" value="`"/>
		<property name="endingDelimiter" value="`"/>

		<plugin type="tk.mybatis.mapper.generator.MapperPlugin">
			<property name="mappers" value="com.yatop.lambda.base.utils.MyMapper"/>
		</plugin>

		<plugin type="org.mybatis.generator.plugins.EqualsHashCodePlugin" />
		<plugin type="org.mybatis.generator.plugins.SerializablePlugin" />
		<plugin type="org.mybatis.generator.plugins.CaseInsensitiveLikePlugin" />
		<plugin type="org.mybatis.generator.plugins.MapperConfigPlugin">
			<property name="targetPackage" value="mybatis" />
			<property name="targetProject" value="./src/main/resources" />
		</plugin>
		<plugin type="com.yatop.lambda.base.plugins.mysql.PaginationPlugin"/>

		<commentGenerator>
			<property name="suppressDate" value="true" />
			<property name="suppressAllComments" value="true" />
		</commentGenerator>

		<jdbcConnection driverClass="${jdbc.driverClassName}"
						connectionURL="${jdbc.jdbcUrl}" userId="${jdbc.username}" password="${jdbc.password}">
		</jdbcConnection>

		<javaModelGenerator targetPackage="com.yatop.lambda.base.model"
							targetProject="./src/main/java">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>

		<sqlMapGenerator targetPackage="com.yatop.lambda.base.sqlMapper"
						 targetProject="./src/main/resources">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>

		<javaClientGenerator targetPackage="com.yatop.lambda.base.mapper"
							 targetProject="./src/main/java"
							 type="XMLMAPPER">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>

		<table tableName="%" >
			<property name="ignoreQualifiersAtRuntime" value="true" />
			<!--<generatedKey column="id" sqlStatement="Mysql" identity="true"/>-->
		</table>
	</context>
</generatorConfiguration>